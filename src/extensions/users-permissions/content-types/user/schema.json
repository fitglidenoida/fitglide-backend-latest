{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "mobile": {
      "type": "biginteger",
      "max": "10"
    },
    "First_name": {
      "type": "string"
    },
    "Last_name": {
      "type": "string"
    },
    "Address_line_1": {
      "type": "string"
    },
    "City": {
      "type": "string"
    },
    "State": {
      "type": "string"
    },
    "Created_date": {
      "type": "datetime"
    },
    "modified_date": {
      "type": "datetime"
    },
    "Activity_selected": {
      "type": "enumeration",
      "enum": [
        "Cardio",
        "Strength ",
        "Cycling",
        "Running"
      ]
    },
    "your_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "workout_plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::workout-plan.workout-plan",
      "mappedBy": "username"
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription",
      "mappedBy": "username"
    },
    "athlete_id": {
      "type": "biginteger"
    },
    "strava_inputs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::strava-input.strava-input",
      "mappedBy": "user"
    },
    "image": {
      "type": "string"
    },
    "health_vitals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::health-vital.health-vital",
      "mappedBy": "username"
    },
    "weightlogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::weightlog.weightlog",
      "mappedBy": "username"
    },
    "authprovider": {
      "type": "enumeration",
      "enum": [
        "FitGlide",
        "Google",
        "Facebook"
      ]
    },
    "sleeplogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sleeplog.sleeplog",
      "mappedBy": "username"
    },
    "diet_plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::diet-plan.diet-plan",
      "mappedBy": "users_permissions_user"
    }
  }
}
