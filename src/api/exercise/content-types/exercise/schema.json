{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "singularName": "exercise",
    "pluralName": "exercises",
    "displayName": "exercise",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "excercise_id": {
      "type": "uid"
    },
    "name": {
      "type": "string",
      "unique": true
    },
    "description": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Cardio",
        "Strength",
        "Flexibility",
        "Core",
        "HIIT"
      ]
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        "Beginner",
        "Intermediate",
        "Advanced"
      ]
    },
    "duration": {
      "type": "decimal"
    },
    "calories_per_minute": {
      "type": "decimal"
    },
    "steps": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "muscle_groups": {
      "type": "enumeration",
      "enum": [
        "Arms",
        "Legs",
        "Core",
        "Back",
        "Chest",
        "Full Body"
      ]
    },
    "diet_components": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::diet-component.diet-component",
      "inversedBy": "exercises"
    },
    "workout_plan": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::workout-plan.workout-plan",
      "inversedBy": "exercises"
    },
    "equipment": {
      "type": "string"
    }
  }
}
